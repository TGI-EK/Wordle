   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" text="Wordle" textAlignment="CENTER">
         <font>
            <Font size="72.0" />
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#onBtnStartAction" text="Start" />
